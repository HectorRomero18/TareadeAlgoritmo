Algoritmo Proyecto
	Definir menuPrincipal,menuNumeros,menuCadenas,menuArreglos,titulo,titulo2,opc,opcn,apcc,apca Como Caracter
	Definir pos,lim Como Entero
	Dimension menuPrincipal[4],menuNumeros[11],menuCadenas[11],menuArreglos[11]
	// Arreglo menu principal
	menuPrincipal[0] = "  1)Ejercicios de Operaciones"
	menuPrincipal[1] = "  2)Ejercicios con Cadenas"
	menuPrincipal[2] = "  3)Ejercicios con Arreglos"
	menuPrincipal[3] = "  4)Salir"
	
	mientras opcion <> '4' hacer
	escribir "Menu principal"
	escribir "1)Operaciones"
	escribir "2)Cadenas"
	escribir "3)Arreglos"
	escribir "4)Salir"
    FinMientras

	Escribir "       Elija opció[1...4,]" Sin Saltar
	leer opcion
	segun opcion hacer
		"1":
		    mientras opc1 <> '00' hacer	
			 escribir "Operaciones"
			 escribir "1) Determinar cuánto se debe pagar por cantidad de colas"
			 escribir "2) Vinicultores"
			 escribir "3) Obtener el residuo sin MOD"
			 escribir "4) Consultorio Dr Paez"
			 escribir "5) Determinar si el primer número es mitad del segundo número y si el tercero es divisor del cuarto"
			 escribir "6) Banco XYZ"
			 escribir "7) Determinar si el primer número es divisor del tercero y si el segundo es el doble del cuarto"
			 escribir "8)Límite de crédito de tarjeta"
			 escribir "9) Mostrar si un número es negativo ,par, impar o múltiplo de 7"
			 escribir "10)Asociación de Vinicultores"
			 escribir "11) Mostrar si un número es menor que 10 par o impar divisible para 5"
			 escribir "12) Fábrica El Cometa"
			 escribir "13) Mostrar dígitos de un numero"
			 escribir "14) Determinar si un número es capicúa"
			 escribir "15) Divisores de un numero"
			 escribir "16) Suma de divisores de un numero"
			 escribir "17) Cantidad de divisores de un numero"
			 escribir "18) Indicar si un número es perfecto"
			 escribir "19) Determinar si un número es primo"
			 escribir "20) Determinar si son números primos gemelos"
			 escribir "21) Determinar si son números primos amigos"
			 escribir "00)Salir"
			 Escribir "       Elija opció[1...21,]" Sin Saltar
			 leer opc1
			 segun opc1
