	Definir contx Como Entero
	Escribir "Ingrese frase o palabra"
	Leer char
	contx=0
	// Funcion con parametros
	contar_x(char,contx)
FinFuncion
Funcion contar_x(char,contx)
	Definir letra Como Caracter
	Definir i Como Entero
	Para i<-0 Hasta Longitud(char) Con Paso 1 Hacer
		letra=Subcadena(char,i,i)
		si letra="x" Entonces
			contx=contx+1
		FinSi
	Fin Para
	Escribir "La frase tiene : ",contx," x"
FinFuncion

//  Leer una secuencia de números hasta que se ingrese un 0
// y almacenarlos en arreglo. Se pide recorrer el arreglo y pasar a otro
// arreglo solo los números pares de cada elemento del arreglo1
// Ejemplo: secuencia: 4,3,6,9,0
// arreglo1=[4,3,6,9] arreglo2= [4,6] 
Funcion ejercicio_33
	Dimension secu[10]
	Dimension parr[10]
	Definir numer,p,i Como Entero
	numer=1; p=1; i=1
	Escribir "**SECUENCIA DE NÚMEROS, MOSTRAR PARES**"
	mientras numer>0 Hacer
		Escribir "Ingrese un número entero (Ingrese 0 para terminar el proceso)"
		Leer numer
		secu[p]=numer
		p=p+1
		Borrar Pantalla
	FinMientras
	p=1
	Escribir "La secuencia de números es:"
	mientras p<=9 Hacer
		si secu[p] > 0 Entonces
			Escribir secu[p]
		FinSi
		p=p+1	
	FinMientras
	p=1
	Escribir "Los números pares son:"
	Mientras p<=9 Hacer
		si secu[p] mod 2 = 0  Entonces
			parr[i]=secu[p]
		FinSi
		p=p+1
		i=i+1
	FinMientras
	Para i <- 1 Hasta 9 Con Paso 1 Hacer
		si parr[i]<>0 Entonces
			Escribir parr[i]
		FinSi
	FinPara
FinFuncion

